#, c-format
msgid "fts_read failed: %s"
msgstr "fts_read schlug fehl: %s"

#: src/du.c:820
#, c-format
msgid "invalid maximum depth %s"
msgstr "Ungültige maximale Tiefe %s"

#: src/du.c:849
