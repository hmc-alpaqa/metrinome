"0\n"
"  [:alnum:]       seluruh huruf dan angka\n"
"  [:alpha:]       seluruh huruf\n"
"  [:blank:]       seluruh horisontal spasi\n"
"  [:cntrl:]       seluruh karakter pengontrol\n"
"  [:digit:]       seluruh digit\n"

#: src/tr.c:329
