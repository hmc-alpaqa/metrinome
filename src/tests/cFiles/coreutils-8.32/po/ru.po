msgstr "длина не кратна 8"

#: src/md5sum.c:956
#, c-format
msgid "maximum digest length for %s is %<PRIuMAX> bits"
msgstr "максимальная длина дайджеста для %s — %<PRIuMAX> бит"

#: src/md5sum.c:974
#, c-format
msgid "--tag does not support --text mode"
msgstr "--tag не поддерживает режим --text"

#: src/md5sum.c:980
#, c-format
