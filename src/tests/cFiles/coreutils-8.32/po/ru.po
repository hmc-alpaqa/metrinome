msgid "FAILED open or read"
msgstr "ОШИБКА при открытии или чтении"

#: src/md5sum.c:790
msgid "FAILED"
msgstr "ПОВРЕЖДЁН"

#: src/md5sum.c:792
msgid "OK"
msgstr "ЦЕЛ"

#: src/md5sum.c:816
#, c-format
