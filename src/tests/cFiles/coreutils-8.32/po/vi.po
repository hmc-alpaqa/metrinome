#: src/mkfifo.c:61 src/mknod.c:63
msgid ""
"  -m, --mode=MODE    set file permission bits to MODE, not a=rw - umask\n"
msgstr ""
"  -m, --mode=CHẾ_ĐỘ  đặt quyền hạn tập tin thành CHẾ_ĐỘ,\n"
"                              thay cho “a=rw - umask”\n"

#: src/mkfifo.c:64 src/mknod.c:66
