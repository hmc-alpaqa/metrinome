       errors=`expr $errors + 1`;;
    2) $echo "Test 13bb.C may have failed." 1>&2
       $echo The command "cmp 13bb.C.O $srcdir/tn_2e8-t_tab" failed. 1>&2
       errors=`expr $errors + 1`;;
  esac
fi
