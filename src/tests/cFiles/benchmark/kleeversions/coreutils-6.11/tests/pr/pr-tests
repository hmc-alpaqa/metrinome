  cmp 4g.C.O $srcdir/a3l15f-t > /dev/null 2>&1
  case $? in
    0) if test "$VERBOSE"; then $echo "passed 4g.C"; fi;;
    1) $echo "Test 4g.C failed: files 4g.C.O and $srcdir/a3l15f-t differ" 1>&2
       (diff -c 4g.C.O $srcdir/a3l15f-t) 2> /dev/null
       errors=`expr $errors + 1`;;
    2) $echo "Test 4g.C may have failed." 1>&2
       $echo The command "cmp 4g.C.O $srcdir/a3l15f-t" failed. 1>&2
       errors=`expr $errors + 1`;;
  esac
