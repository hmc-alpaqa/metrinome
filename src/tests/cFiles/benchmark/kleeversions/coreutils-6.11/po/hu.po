msgstr "érvénytelen egész argumentum: %s"

#: src/su.c:241
msgid "Password:"
msgstr "Jelszó:"

#: src/su.c:244
#, c-format
msgid "getpass: cannot open /dev/tty"
msgstr "getpass: a /dev/tty nem nyitható meg"

#: src/su.c:302
