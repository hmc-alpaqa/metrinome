msgstr "för många mallar"

#: src/mktemp.c:216
#, c-format
msgid "too few X's in template %s"
msgstr "för få X i mallen %s"

#: src/mktemp.c:229
#, c-format
msgid "invalid template, %s, contains directory separator"
msgstr "ogiltig mall, %s, innehåller katalogseparator"

#: src/mktemp.c:243
#, c-format
